    "react": "^18.0.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^18.0.0",
    "react-dropzone": "^14.0.0",
    "react-hook-form": "^7.33.0",
    "react-measure": "^2.5.2",
    "react-router-dom": "^6.0.1",
    "starknet": "^3.16.1",
    "starknet-390": "npm:starknet@3.9.0",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7",
    "swr": "^1.3.0",
    "tracking-link": "^4.0.1",
    "url-join": "^5.0.0",
    "webextension-polyfill": "^0.9.0",
    "yup": "^1.0.0-beta.4",
    "zustand": "^3.6.5"
  }
